{
  "name": "taiga",
  "version": "0.0.0",
  "license": "MPL-2",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build && npm run transloco:optimize",
    "default-config": "cp config.example.json apps/taiga/src/assets/config.json",
    "test": "nx run-many --all --target=test",
    "lint": "nx workspace-lint && npm run lint:angular && npm run lint:html && npm run lint:css",
    "lint:affected": "nx workspace-lint && npm run affected:lint:angular && npm run lint:html && npm run lint:css",
    "lint:angular": "nx run-many --all --target=lint",
    "lint:html": "npx htmlhint 'apps/**/*.html' 'libs/**/*.html'",
    "lint:css": "npx stylelint 'apps/**/*.css' 'libs/**/*.css'",
    "e2e": "nx e2e taiga-e2e",
    "e2e:admin": "nx e2e taiga-e2e --watch",
    "affected:lint:angular": "nx run-many --all --target=affected:lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:write:all": "nx format:write --all",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:ssr": "ng run taiga:serve-ssr",
    "serve:ssr": "node dist/taiga/server/main.js",
    "storybook": "nx run ui:storybook",
    "storybook:build": "nx build-storybook ui",
    "transloco:optimize": "transloco-optimize dist/taiga/browser/assets/i18n",
    "build:prod": "ng build --prod && npm run transloco:optimize",
    "build:ssr": "npm run build:prod && ng run taiga:server:production",
    "prerender": "ng run taiga:prerender",
    "translations:split": "ng g @ngneat/transloco:split --translation-path 'apps/taiga/src/assets/i18n/'",
    "translations:join": "ng g @ngneat/transloco:join --translation-path 'apps/taiga/src/assets/i18n/'",
    "install:hooks": "cd .. && husky install javascript/.husky",
    "prepare": "npm run install:hooks",
    "emails:compile-html": "npx mjml --config.keepComments=0 ./apps/taiga-mjml/templates/**/*.mjml.html.jinja -o ../python/apps/taiga/src/taiga/emails/templates",
    "emails:copy-text": "npx copyfiles -f ./apps/taiga-mjml/templates/**/*.txt.jinja ../python/apps/taiga/src/taiga/emails/templates",
    "emails:copy-subject": "npx copyfiles -f ./apps/taiga-mjml/templates/**/*.subject.jinja ../python/apps/taiga/src/taiga/emails/templates",
    "emails:copy-static": "npx copyfiles -f ./apps/taiga-mjml/static/emails/**/*.* ../python/apps/taiga/src/taiga/emails/static/emails",
    "emails": "npm run emails:compile-html && npm run emails:copy-text && npm run emails:copy-subject && npm run emails:copy-static  && node ./scripts/append-license.js",
    "email:serve": "node apps/email-server/index.js"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics-cli": "~14.0.0",
    "@angular/animations": "14.1.0",
    "@angular/cdk": "~14.0.0",
    "@angular/common": "14.1.0",
    "@angular/compiler": "14.1.0",
    "@angular/core": "14.1.0",
    "@angular/forms": "14.1.0",
    "@angular/platform-browser": "14.1.0",
    "@angular/platform-browser-dynamic": "14.1.0",
    "@angular/platform-server": "14.1.0",
    "@angular/router": "14.1.0",
    "@ngneat/transloco": "^4.1.1",
    "@ngneat/transloco-messageformat": "^4.0.0",
    "@ngneat/until-destroy": "^9.2.1",
    "@ngrx/effects": "~14.0.0",
    "@ngrx/router-store": "~14.0.0",
    "@ngrx/store": "~14.0.0",
    "@ngrx/store-devtools": "14.0.0",
    "@nguniversal/express-engine": "^14.0.3",
    "@nrwl/angular": "14.5.1",
    "@rx-angular/cdk": "^1.0.0-beta.3",
    "@rx-angular/state": "^1.6.1",
    "@rx-angular/template": "^1.0.0-beta.33",
    "@taiga-ui/addon-table": "^2.55.0",
