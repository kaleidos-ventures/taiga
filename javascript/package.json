    "@tinkoff/ng-dompurify": "^3.0.0",
    "@tinkoff/ng-event-plugins": "^3.0.0",
    "change-case": "^4.1.2",
    "check-password-strength": "^2.0.7",
    "diacritic": "^0.0.2",
    "eslint-plugin-taiga": "file:eslint-plugin-taiga",
    "hotkeys-js": "^3.9.4",
    "immer": "^9.0.15",
    "messageformat": "^2.3.0",
    "ngx-quicklink": "^0.3.0",
    "rxjs": "~7.5.0",
    "smtp-server": "^3.11.0",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2",
    "zone.js": "0.11.7"
  },
